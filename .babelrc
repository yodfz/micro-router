{
  "plugins": [
    [
      // 先进的代码兼容插件
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3,
        "useESModules": true
      }
    ]
  ],
  "presets": [
    [
      // 先进的代码兼容插件
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          // 仅做chrome兼容
          "chrome": "70"
        }
      }
    ],
    // react转换插件
    "@babel/preset-react",
    // ts转换插件
    "@babel/preset-typescript"
  ]
}
